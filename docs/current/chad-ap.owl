        <vaem:rationale xml:lang="en">This class represents organisations and other entities made up by groups of actors.</vaem:rationale>
        <rdfs:comment xml:lang="en">A unified group of people.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.cidoc-crm.org/cidoc-crm/"/>
