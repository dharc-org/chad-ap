    <!-- http://vocab.getty.edu/aat/300011172 -->


    <owl:NamedIndividual rdf:about="http://vocab.getty.edu/aat/300011172">
        <rdf:type rdf:resource="http://vocab.getty.edu/aat/Concept"/>
