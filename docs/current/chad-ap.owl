        ex:processing a crmdig:D10_Software_Execution ;
            crm:P2_has_type aat:300054636 ;
            crmdig:L10_had_input ex:data-object .

    </vann:example>
        <vaem:rationale xml:lang="en">This class is used to represent the DCHO produced at the end of each digitisation activity.</vaem:rationale>
        <rdfs:comment xml:lang="en">A digital object that is the direct result of an activity in a digitization workflow.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ics.forth.gr/isl/CRMdig/"/>
