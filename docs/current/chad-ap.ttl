##################################################################

crm:E42_Identifier 
    a owl:Class ;
    rdfs:label "Identifier"@en,
                "Kennung"@de,
                "Κωδικός Αναγνώρισης"@el,
                "Identifiant"@fr,
                "Identificador de Objeto"@pt,
                "Идентификатор Объекта"@ru,
                "标识符"@zh ;
    rdfs:comment "A textual string assigned to a cultural heritage object in order to identify it uniquely within a given context."@en ;
    rdfs:subClassOf crm:E41_Appellation ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

frbroo:F4_Manifestation_Singleton 
    a owl:Class ;
    rdfs:label "Manifestation Singleton"@en ;
    rdfs:comment "The physical instance of a cultural heritage object."@en ;
    rdfs:subClassOf crm:E24_Physical_Man-Made_Thing ;
    rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frbr/frbroo/> .

crm:E35_Title 
    a owl:Class ;
    rdfs:label "Title"@en,
                "Titel"@de,
                "Τίτλος"@el,
                "Titre"@fr,
                "Título"@pt,
                "Заголовок"@ru,
                "题名"@zh ;
    rdfs:comment "A textual string that functions as a title for a work."@en ;
    rdfs:subClassOf crm:E33_Linguistic_Object, 
        crm:E41_Appellation ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:E87_Curation_Activity 
    a owl:Class ;
    rdfs:label "Curation Activity"@en,
                "Kuratorische Tätigkeit"@de,
                "Activité curatoriale"@fr,
                "管理"@zh ;
    rdfs:comment "An activity that results in the continuity of management and the preservation and evolution of a collection."@en ;
    rdfs:subClassOf crm:E7_Activity ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:E78_Curated_Holding 
    a owl:Class ;
    rdfs:label "Curated Holding"@en,
