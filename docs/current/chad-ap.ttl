@prefix crm: <http://www.cidoc-crm.org/cidoc-crm/> .
@prefix crmdig: <http://www.ics.forth.gr/isl/CRMdig/> .
@prefix frbroo: <http://iflastandards.info/ns/fr/frbr/frbroo/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://w3id.org/dharc/ontology/chad-ap/> rdf:type owl:Ontology .

##################################################################
# CLASSES ########################################################
##################################################################

crm:E42_Identifier 
    a owl:Class ;
    rdfs:label "Identifier"@en,
                "Kennung"@de,
                "Κωδικός Αναγνώρισης"@el,
                "Identifiant"@fr,
                "Identificador de Objeto"@pt,
                "Идентификатор Объекта"@ru,
                "标识符"@zh ;
    rdfs:comment "A textual string assigned to a cultural heritage object in order to identify it uniquely within a given context."@en ;
    rdfs:subClassOf crm:E41_Appellation ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

frbroo:F4_Manifestation_Singleton 
    a owl:Class ;
    rdfs:label "Manifestation Singleton"@en ;
    rdfs:comment "The physical instance of a cultural heritage object."@en ;
    rdfs:subClassOf crm:E24_Physical_Man-Made_Thing ;
    rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frbr/frbroo/> .

crm:E35_Title 
    a owl:Class ;
    rdfs:label "Title"@en,
                "Titel"@de,
                "Τίτλος"@el,
                "Titre"@fr,
                "Título"@pt,
                "Заголовок"@ru,
                "题名"@zh ;
    rdfs:comment "A textual string that functions as a title for a work."@en ;
    rdfs:subClassOf crm:E33_Linguistic_Object, 
        crm:E41_Appellation ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:E87_Curation_Activity 
    a owl:Class ;
    rdfs:label "Curation Activity"@en,
                "Kuratorische Tätigkeit"@de,
                "Activité curatoriale"@fr,
                "管理"@zh ;
    rdfs:comment "An activity that results in the continuity of management and the preservation and evolution of a collection."@en ;
    rdfs:subClassOf crm:E7_Activity ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:E78_Curated_Holding 
    a owl:Class ;
    rdfs:label "Curated Holding"@en,
                "Collection"@fr ;
    rdfs:comment "A collection of physical objects that is assembled and maintained by one or more actors over time for a specific purpose and audience."@en ;
    rdfs:subClassOf crm:E24_Physical_Human-Made_Thing ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:E52_Time-Span 
    a owl:Class ;
    rdfs:label "Time-Span"@en,
                "Zeitspanne"@de,
                "Χρονικό Διάστημα"@el,
                "Intervalle temporel"@fr,
                "Período de Tempo"@pt,
                "Интервал Времени"@ru,
                "时段"@zh ;
    rdfs:comment "An abstract temporal extent having a beginning, an end and a duration."@en ;
    rdfs:subClassOf crm:E1_CRM_Entity ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:E53_Place 
    a owl:Class ;
    rdfs:label "Place"@en,
                "Ort"@de,
                "Τόπος"@el,
                "Lieu"@fr,
                "Local"@pt,
                "Место"@ru,
                "地点"@zh ;
    rdfs:comment "A geographical location."@en ;
    rdfs:subClassOf crm:E1_CRM_Entity ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:E39_Actor 
    a owl:Class ;
    rdfs:label "Actor"@en,
                "Akteur"@de,
                "Δράστης"@el,
                "Actant"@fr,
                "Agente"@pt,
                "Агент"@ru,
                "参与者"@zh ;
    rdfs:comment "An individual person or a group of people, with the potential to perform intentional and accountable actions."@en ;
    rdfs:subClassOf crm:E77_Persistent_Item ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

frbroo:F27_Work_Conception 
    a owl:Class ;
    rdfs:label "Work Conception"@en ;
    rdfs:comment "An activity that represents the creation of a work."@en ;
    rdfs:subClassOf crm:E65_Creation ;
    rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frbr/frbroo/> .

frbroo:F1_Work 
    a owl:Class ;
    rdfs:label "Work"@en ;
    rdfs:comment "The essence or conceptualization of a cultural heritage object."@en ;
    rdfs:subClassOf crm:E89_Propositional_Object ;
    rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frbr/frbroo/> .

frbroo:F28_Expression_Creation 
    a owl:Class ;
    rdfs:label "Expression Creation"@en ;
    rdfs:comment "An activity that represents the creation of an expression."@en ;
    rdfs:subClassOf crm:E12_Production ;
    rdfs:subClassOf crm:E65_Creation ;
    rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frbr/frbroo/> .

crm:E7_Activity 
    a owl:Class ;
    rdfs:label "Activity"@en,
                "Handlung"@de,
                "Δράση"@el,
                "Activité"@fr,
                "Atividade"@pt,
                "Деятельность"@ru,
                "活动"@zh ;
    rdfs:comment "An action intentionally carried out by one or more actors."@en ;
    rdfs:subClassOf crm:E5_Event ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:E55_Type 
    a owl:Class ;
    rdfs:label "Type"@en,
                "Typus"@de,
                "Τύπος"@el,
                "Type"@fr,
                "Tipo"@pt,
                "Тип"@ru,
                "类型"@zh ;
    rdfs:comment "A categorical concept denoted by terms from thesauri and controlled vocabularies used to characterize and classify other entities."@en ;
    rdfs:subClassOf crm:E28_Conceptual_Object ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

frbroo:F2_Expression 
    a owl:Class ;
    rdfs:label "Expression"@en ;
    rdfs:comment "The realisation of a work, referring to the intellectual content of a cultural heritage object."@en ;
    rdfs:subClassOf crm:E73_Information_Object ;
    rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frbr/frbroo/> .

crm:E73_Information_Object 
    a owl:Class ;
    rdfs:label "Information Object"@en,
                "Informationsgegenstand"@de,
                "Πληροφοριακό Αντικείμενο"@el,
                "Objet informationnel"@fr,
                "Objeto de Informação"@pt,
                "Информационный Объект"@ru,
                "信息对象"@zh ;
    rdfs:comment "An immaterial item that has an objectively recognizable structure and is documented as a single unit."@en ;
    rdfs:subClassOf crm:E89_Propositional_Object,
                    crm:E90_Symbolic_Object ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:E31_Document 
    a owl:Class ;
    rdfs:label "Document"@en,
                "Dokument"@de,
                "Τεκμήριο"@el,
                "Document"@fr,
                "Documento"@pt,
                "Документ"@ru,
                "文献"@zh ;
    rdfs:comment "An information object that makes propositions about reality."@en ;
    rdfs:subClassOf crm:E73_Information_Object ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

frbroo:F15_Complex_Work 
    a owl:Class ;
    rdfs:label "Complex Work"@en ;
    rdfs:comment "A work that is composed of other works."@en ;
    rdfs:subClassOf frbroo:F1_Work ;
    rdfs:isDefinedBy <http://iflastandards.info/ns/fr/frbr/frbroo/> .

crm:E24_Physical_Human-Made_Thing
    a owl:Class ;
    rdfs:label "Physical Human-Made Thing"@en ,
        "Chose matérielle élaborée par l’humain"@fr ;
    rdfs:comment "A persistent physical item of any size that is purposely created by human activity."@en ;
    rdfs:subClassOf crm:E18_Physical_Thing ;
    rdfs:subClassOf crm:E71_Human-Made_Thing ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crmdig:D10_Software_Execution
    a owl:Class ;
    rdfs:label "Software Execution"@en ;
    rdfs:comment "An activity in which a digital device runs a software program on a digital object."@en ;
    rdfs:subClassOf crmdig:D7_Digital_Machine_Event ;
    rdfs:isDefinedBy <http://www.ics.forth.gr/isl/CRMdig/> .

crmdig:D14_Software
    a owl:Class ;
    rdfs:label "Software"@en ;
    rdfs:comment "A computer program."@en ;
    rdfs:subClassOf crmdig:D1_Digital_Object ;
    rdfs:isDefinedBy <http://www.ics.forth.gr/isl/CRMdig/> .

crmdig:D8_Digital_Device
    a owl:Class ;
    rdfs:label "Digital Device"@en ;
    rdfs:comment "A technical tool that is used to process or produce digital objects."@en ;
    rdfs:subClassOf crm:E22_Man-Made_Object ;
    rdfs:isDefinedBy <http://www.ics.forth.gr/isl/CRMdig/> .

crmdig:D2_Digitization_Process
    a owl:Class ;
    rdfs:label "Digitization Process"@en ;
    rdfs:comment "An activity that results in the production of data objects functioning as digital representations of physical entities."@en ;
    rdfs:subClassOf crmdig:D11_Digital_Measurement_Event ;
    rdfs:isDefinedBy <http://www.ics.forth.gr/isl/CRMdig/> .

crm:E21_Person
    a owl:Class ;
    rdfs:label "Person"@en ,
        "Person"@de ,
        "Πρόσωπο"@el ,
        "Personne"@fr ,
        "Pessoa"@pt ,
        "Личность"@ru ,
        "人物"@zh ;
    rdfs:comment "An individual person."@en ;
    rdfs:subClassOf crm:E20_Biological_Object ,
        crm:E39_Actor ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:E74_Group
    a owl:Class ;
    rdfs:label "Group"@en ,
        "Menschliche Gruppe"@de ,
        "Ομάδα"@el ,
        "Groupe"@fr ,
        "Grupo"@pt ,
        "Группа"@ru ,
        "团体"@zh ;
    rdfs:comment "A unified group of people."@en ;
    rdfs:subClassOf crm:E39_Actor ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crmdig:D9_Data_Object
    a owl:Class ;
    rdfs:comment "A digital object that is the direct result of an activity in a digitization workflow."@en ;
    rdfs:subClassOf crm:E54_Dimension ;
    rdfs:subClassOf crmdig:D1_Digital_Object ;
    rdfs:isDefinedBy <http://www.ics.forth.gr/isl/CRMdig/> .

##################################################################
# OBJECT PROPERTIES ##############################################
##################################################################

crm:P48_has_preferred_identifier 
    a owl:ObjectProperty ;
    rdfs:label "has preferred identifier"@en,
                "hat bevorzugtes Kennzeichen"@de,
                "έχει προτιμώμενο αναγνωριστικό"@el,
                "a pour identifiant préférentiel"@fr,
                "tem identificador preferido"@pt,
                "имеет предпочтительный идентификатор"@ru,
                "有优选标识符"@zh ;
    rdfs:comment "This property records the preferred identifier used to identify an entity."@en ;
    rdfs:domain crm:E1_CRM_Entity ;
    rdfs:range crm:E42_Identifier ;
    rdfs:subPropertyOf crm:P1_is_identified_by ;
    owl:inverseOf crm:P48i_is_preferred_identifier_of ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:P48i_is_preferred_identifier_of 
    a owl:ObjectProperty ;
    rdfs:label "is preferred identifier of"@en,
                "ist bevorzugtes Kennzeichen für"@de,
                "είναι προτιμώμενο αναγνωριστικό"@el,
                "est l’identifiant préférentiel de"@fr,
                "é o identificador preferido de"@pt,
                "является предпочтительным идентификатором для"@ru,
                "是优选标识符"@zh ;
    rdfs:domain crm:E42_Identifier ;
    rdfs:range crm:E1_CRM_Entity ;
    rdfs:subPropertyOf crm:P1i_identifies ;
    owl:inverseOf crm:P48_has_preferred_identifier ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:P46_is_composed_of 
    a owl:ObjectProperty ;
    rdfs:label "is composed of"@en,
                "ist zusammengesetzt aus"@de,
                "αποτελείται από"@el,
                "est composé de"@fr,
                "é composto de"@pt,
                "составлен из"@ru,
                "组成成分是"@zh ;
    rdfs:comment "This property associates a physical entity with another physical entity that forms part of it."@en ;
    rdfs:domain crm:E18_Physical_Thing ;
    rdfs:range crm:E18_Physical_Thing ;
    owl:inverseOf crm:P46i_forms_part_of ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:P46i_forms_part_of 
    a owl:ObjectProperty ;
    rdfs:label "forms part of"@en,
                "bildet Teil von"@de,
                "αποτελεί μέρος του/της"@el,
                "fait partie de"@fr,
                "faz parte de"@pt,
                "формирует часть"@ru,
                "构成部分"@zh ;
    rdfs:domain crm:E18_Physical_Thing ;
    rdfs:range crm:E18_Physical_Thing ;
    owl:inverseOf crm:P46_is_composed_of ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:P2_has_type 
    a owl:ObjectProperty ;
    rdfs:label "has type"@en ,
                "hat den Typus"@de ,
                "έχει τύπο"@el ,
                "a pour type"@fr ,
                "é do tipo"@pt ,
                "имеет тип"@ru ,
                "有类型"@zh ;
    rdfs:comment "This property allows typing of an entity through the use of an external knowledge organization system, such as a thesaurus or a controlled vocabulary."@en ;
    rdfs:domain crm:E1_CRM_Entity ;
    rdfs:range crm:E55_Type ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:P12_occurred_in_the_presence_of 
    a owl:ObjectProperty ;
    rdfs:label "occurred in the presence of"@en,
                "fand statt im Beisein von"@de,
                "συνέβη παρουσία του/της"@el,
                "a eu lieu en présence de"@fr,
                "ocorreu na presença de"@pt,
                "появился в присутствии"@ru,
                "已出现"@zh ;
    rdfs:comment "This property describes the presence of an entity during an event, without implying any specific role."@en ;
    rdfs:domain crm:E5_Event ;
    rdfs:range crm:E77_Persistent_Item ;
    owl:inverseOf crm:P12i_was_present_at ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:P12i_was_present_at 
    a owl:ObjectProperty ;
    rdfs:label "was present at"@en,
                "war anwesend bei"@de,
                "ήταν παρών/παρούσα/παρόν σε"@el,
                "a été présent à"@fr,
                "estava presente no"@pt,
                "присутствовал при"@ru,
                "出现在"@zh ;
    rdfs:domain crm:E77_Persistent_Item ;
    rdfs:range crm:E5_Event ;
    owl:inverseOf crm:P12_occurred_in_the_presence_of ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:P49_has_former_or_current_keeper 
    a owl:ObjectProperty ;
    rdfs:label "has former or current keeper"@en,
                "hat früheren oder derzeitigen Betreuer"@de,
                "είναι ή ήταν στην κατοχή του"@el,
                "a pour actant détenteur actuel ou antérieur"@fr,
                "é ou foi guardada por"@pt,
                "имеет бывшего или текущего смотрителя"@ru,
                "有以往或当前保管者"@zh ;
    rdfs:comment "This property identifies an actor who has or has had custody of a physical entity at some time."@en ;
    rdfs:domain crm:E18_Physical_Thing ;
    rdfs:range crm:E39_Actor ;
    owl:inverseOf crm:P49i_is_former_or_current_keeper_of ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:P49i_is_former_or_current_keeper_of 
    a owl:ObjectProperty ;
    rdfs:label "is former or current keeper of"@en,
                "ist früherer oder derzeitiger Betreuer von"@de,
                "κατέχει ή κατείχε"@el,
                "est l’actant détenteur actuel ou antérieur de"@fr,
                "é ou foi guardador de"@pt,
                "является бывшим или текущим смотрителем для"@ru,
                "是以往或当前保管者"@zh ;
    rdfs:domain crm:E39_Actor ;
    rdfs:range crm:E18_Physical_Thing ;
    owl:inverseOf crm:P49_has_former_or_current_keeper ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:P147_curated 
    a owl:ObjectProperty ;
    rdfs:label "curated"@en,
                "betreute kuratorisch"@de,
                "a géré"@fr,
                "管理"@zh ;
    rdfs:comment "This property associates a curation activity with a collection involved in such activity."@en ;
    rdfs:domain crm:E87_Curation_Activity ;
    rdfs:range crm:E78_Curated_Holding ;
    owl:inverseOf crm:P147i_was_curated_by ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:P147i_was_curated_by 
    a owl:ObjectProperty ;
    rdfs:label "was curated by"@en,
                "wurde kuratorisch betreut durch"@de,
                "a été géré par"@fr,
                "被管理"@zh ;
    rdfs:domain crm:E78_Curated_Holding ;
    rdfs:range crm:E87_Curation_Activity ;
    owl:inverseOf crm:P147_curated ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:P102_has_title 
    a owl:ObjectProperty ;
    rdfs:label "has title"@en,
                "trägt den Titel"@de,
                "έχει τίτλο"@el,
                "a pour titre"@fr,
                "tem título"@pt,
                "имеет заголовок"@ru,
                "有题名"@zh ;
    rdfs:comment "This property associates a title with the work bearing that title."@en ;
    rdfs:domain crm:E71_Human-Made_Thing ;
    rdfs:range crm:E35_Title ;
    rdfs:subPropertyOf crm:P1_is_identified_by ;
    owl:inverseOf crm:P102i_is_title_of ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:P102i_is_title_of 
    a owl:ObjectProperty ;
    rdfs:label "is title of"@en,
                "ist der Titel von"@de,
                "είναι τίτλος του/της"@el,
                "est le titre de"@fr,
                "é título de"@pt,
                "является заголовком для"@ru,
                "题名是"@zh ;
    rdfs:domain crm:E35_Title ;
    rdfs:range crm:E71_Human-Made_Thing ;
    rdfs:subPropertyOf crm:P1i_identifies ;
    owl:inverseOf crm:P102_has_title ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:P4_has_time-span 
    a owl:ObjectProperty ;
    rdfs:label "has time-span"@en,
                "hat Zeitspanne"@de,
                "βρισκόταν σε εξέλιξη"@el,
                "a pour intervalle temporel"@fr,
                "tem período de tempo"@pt,
                "имеет временной отрезок"@ru,
                "发生时段是"@zh ;
    rdfs:comment "This property associates a phenomenon with the time span during which that phenomenon was on-going."@en ;
    rdfs:domain crm:E2_Temporal_Entity ;
    rdfs:range crm:E52_Time-Span ;
    owl:inverseOf crm:P4i_is_time-span_of ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:P4i_is_time-span_of 
    a owl:ObjectProperty ;
    rdfs:label "is time-span of"@en,
                "ist Zeitspanne von"@de,
                "είναι χρονικό διάστημα του/της"@el,
                "est l’intervalle temporel de"@fr,
                "é o período de tempo de"@pt,
                "является временным отрезком для"@ru,
                "是时段"@zh ;
    rdfs:domain crm:E52_Time-Span ;
    rdfs:range crm:E2_Temporal_Entity ;
    owl:inverseOf crm:P4_has_time-span ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:P14_carried_out_by 
    a owl:ObjectProperty ;
    rdfs:label "carried out by"@en,
                "wurde ausgeführt von"@de,
                "πραγματοποιήθηκε από"@el,
                "a été effectué par"@fr,
                "realizada por"@pt,
                "выполнялся"@ru,
                "执行者是"@zh ;
    rdfs:comment "This property describes the active, accountable participation of an actor during an activity."@en ;
    rdfs:domain crm:E7_Activity ;
    rdfs:range crm:E39_Actor ;
    rdfs:subPropertyOf crm:P11_had_participant ;
    owl:inverseOf crm:P14i_performed ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:P14i_performed 
    a owl:ObjectProperty ;
    rdfs:label "performed"@en,
                "führte aus"@de,
                "πραγματοποίησε"@el,
                "a effectué"@fr,
                "executou"@pt,
                "выполнял"@ru,
                "执行"@zh ;
    rdfs:domain crm:E39_Actor ;
    rdfs:range crm:E7_Activity ;
    rdfs:subPropertyOf crm:P11i_participated_in ;
    owl:inverseOf crm:P14_carried_out_by ;
    rdfs:isDefinedBy <http://www.cidoc-crm.org/cidoc-crm/> .

crm:P74_has_current_or_former_residence 
    a owl:ObjectProperty ;
    rdfs:label "has current or former residence"@en,
                "hat derzeitigen oder früheren Sitz"@de,
                "έχει ή είχε κατοικία"@el,
                "a pour résidence actuelle ou antérieure"@fr,
                "reside ou residiu em"@pt,
