ex:ALD-L1-curation a obj:CurationEvent ;
                rdfs:label "Curation event of 'Amerigo Vespucci sveglia l America'"@en ;
                obj:curates ex:nova-reperta ;
                obj:isCarriedOutBy ex:rijksmuseum ;
                obj:occurredInThePresenceOf ex:ALD-L1-man .

ex:ALD-L1-man a obj:CulturalResource ;
                rdfs:label "Amerigo Vespucci sveglia l'America"@en ;
                obj:hasCurator ex:rijksmuseum .

ex:nova-reperta a obj:Collection ;
                rdfs:label "Nova Reperta" .

ex:rijksmuseum a obj:Agent ;
                rdfs:label "Rijksmuseum" ;
                obj:hasPlace ex:amsterdam .

ex:amsterdam a obj:Place ;
               rdfs:label "Amsterdam" .

ex:ALD-1-curation a obj:CurationEvent ;
                rdfs:label "Curation event of 'Panthera leo (Linnaeus, 1758)'"@en ;
