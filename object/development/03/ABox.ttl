                obj:isCarriedOutBy ex:sistema-museale-di-ateneo ;
                obj:occurredInThePresenceOf ex:ALD-1-man .

ex:ALD-1-man a obj:CulturalResource ;
                rdfs:label "Panthera leo (Linnaeus, 1758)"@en ;
                obj:hasCurator ex:sistema-museale-di-ateneo .

ex:collezione-di-zoologia a obj:Collection ;
                rdfs:label "Collezione di Zoologia" .

ex:sistema-museale-di-ateneo a obj:Agent ;
                rdfs:label "Sistema museale di Ateneo" ;
                obj:hasPlace ex:bologna .

ex:bologna a obj:Place ;
               rdfs:label "Bologna" .

ex:ALD-14-curation a obj:CurationEvent ;
                rdfs:label "Curation event of 'Limulus polyphemus (Linnaeus, 1758)'"@en ;
                obj:curates ex:collezione-di-anatomia-comparata ;
                obj:isCarriedOutBy ex:sistema-museale-di-ateneo ;
                obj:occurredInThePresenceOf ex:ALD-14-man .

ex:ALD-14-man a obj:CulturalResource ;
                rdfs:label "Limulus polyphemus (Linnaeus, 1758)"@en ;
