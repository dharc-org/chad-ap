@prefix ex: <http://purl.org/changes/process/development/02/data/> .
@prefix process: <http://purl.org/changes/process/development/02/schema/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://purl.org/changes/process/development/02/data/> .

<http://purl.org/changes/process/development/02/data/> rdf:type owl:Ontology ;
    owl:imports <http://purl.org/changes/process/development/02/schema/> .

ex:ALD-15-acq-output a process:DigitalObject;
                rdfs:label "Output of the Digitization process of Corno Lavorato"@en .

ex:ALD-15-proc-output a process:DigitalObject ;
                rdfs:label "Output of the Processing activity of Corno Lavorato"@en .

ex:ALD-15-acq a process:DigitizationProcess ;
                rdfs:label "Digitization of Corno Lavorato"@en ;
                process:hasOutput ex:ALD-15-acq-output .

ex:ALD-15-proc a process:SoftwareActivity ;
                rdfs:label "Processing activity of Corno Lavorato"@en ;
                process:hasType process:processing ;
                process:hasInput ex:ALD-15-acq-output ;
                process:hasOutput ex:ALD-15-proc-output ;
                process:carriedOutBy ex:federica-collina ;
                process:hasParticipant ex:unibo-dbc .

ex:federica-collina a process:Person ;
                rdfs:label "Federica Collina" .

ex:unibo-dbc a process:Institution ;
                rdfs:label "UNIBO DBC" .