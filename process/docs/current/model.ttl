    rdfs:label "is referred to by"@en ;
    rdfs:label "wird angeführt von"@de ;
    rdfs:label "αναφέρεται από"@el ;
    rdfs:label "fait l'objet d'un renvoi par"@fr ;
    rdfs:label "é referenciado por"@pt ;
    rdfs:label "имеет ссылку на себя от"@ru ;
    rdfs:label "被涉及"@zh ;
