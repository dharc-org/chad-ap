    rdfs:label "est documenté dans"@fr ;
    rdfs:label "é documentado em"@pt ;
    rdfs:label "документирован в"@ru ;
    rdfs:label "记录在"@zh ;
    rdfs:domain crm:E1_CRM_Entity ;
